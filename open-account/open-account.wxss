page{
    background:#fff;
    font-family: "微软雅黑";
}
.openList{
    height:220rpx;
    border-bottom:1px solid #e0e0e0;
}
.openList .openLogo{
    width:230rpx;
    height:130rpx;
    float:left;
    margin:50rpx 25rpx 0;
}
.openList .openCon{
    width:445rpx;
    float:left;
    padding-top:47rpx;
}
.marbott{
    width:100%;
    height:22rpx;
    background:#f5f5f5;
    border-bottom:1px solid #e0e0e0;
}
.openList .openCon .openTop .jys{
    font-size:26rpx;
}
.openList .openCon .openTop .jysBtn{
    font-size:24rpx;
    color:#ed3f48;
    border:1px solid #ed3f48;
    border-radius:8rpx;
    width:110rpx;
    text-align:center;
    height:36rpx;
    line-height:36rpx;
    float:right;
    margin-top:10rpx;
}
.openList .openCon .jysName{
    font-size:22rpx;
    color:#999;
    letter-spacing: 5rpx;
    margin-top:55rpx;
}